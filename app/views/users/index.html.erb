<h1>All Users.</h1>

<table>
  <thead>
    <tr>
      <th colspan="3">id</th>
      <th colspan="3">Name</th>
      <th colspan="3">Ref</th>
      <th colspan="3">Detail</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td colspan="3"><%= user.id %></td>
        <td colspan="3"><%= user.name %></td>
        <td colspan="3"><%= user.ref %></td>
        <td colspan="3"><%= user.detail %></td>
      </tr>
    <% end %>
  </tbody>
</table>
